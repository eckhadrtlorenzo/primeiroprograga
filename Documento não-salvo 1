// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int paulo;
    int jose;
    int soma;
    paulo = -54;
    jose = 24;
    soma = paulo + jose; 
    printf("o valor de paulo é %d. \n" , paulo);
    printf("o valor de jose é %d. \n " , jose );
    printf("a soma dos dois numeros é %d ", soma);

    return 0;
}
